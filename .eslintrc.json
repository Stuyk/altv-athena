{
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint/eslint-plugin",
    "vue",
    "prettier"
  ],
  "overrides": [
    {
      "files": [
        "*.js"
      ],
      "parserOptions": {
        "project": ["./tsconfig.json"]
      }
    }
  ],
  "parserOptions": {
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "Vue": true,
    "Vuetify": true,
    "YT": true,
    "alt": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "off",
    "no-empty": [
      "error",
      {
        "allowEmptyCatch": true
      }
    ]
  }
}